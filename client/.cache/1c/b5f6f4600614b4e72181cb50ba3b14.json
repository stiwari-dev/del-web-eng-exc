ine":1,"column":4932}},{"source":"views/furniture.js","name":"renderFurnituresListPage","original":{"line":128,"column":40},"generated":{"line":1,"column":4957}},{"source":"views/furniture.js","name":"renderFurniturePage","original":{"line":128,"column":66},"generated":{"line":1,"column":4959}},{"source":"views/furniture.js","name":"renderFurniturePage","original":{"line":128,"column":66},"generated":{"line":1,"column":4979}},{"source":"views/furniture.js","name":"showError","original":{"line":128,"column":87},"generated":{"line":1,"column":4981}},{"source":"views/furniture.js","name":"showError","original":{"line":128,"column":87},"generated":{"line":1,"column":4991}},{"source":"views/furniture.js","name":"showLoader","original":{"line":128,"column":98},"generated":{"line":1,"column":4993}},{"source":"views/furniture.js","name":"showLoader","original":{"line":128,"column":98},"generated":{"line":1,"column":5004}},{"source":"views/furniture.js","name":"hideLoader","original":{"line":128,"column":110},"generated":{"line":1,"column":5006}},{"source":"views/furniture.js","name":"hideLoader","original":{"line":128,"column":110},"generated":{"line":1,"column":5017}},{"source":"views/furniture.js","name":"showSearch","original":{"line":128,"column":122},"generated":{"line":1,"column":5019}},{"source":"views/furniture.js","name":"showSearch","original":{"line":128,"column":122},"generated":{"line":1,"column":5030}},{"source":"views/furniture.js","name":"hideSearch","original":{"line":128,"column":134},"generated":{"line":1,"column":5032}},{"source":"views/furniture.js","name":"hideSearch","original":{"line":128,"column":134},"generated":{"line":1,"column":5043}},{"source":"views/furniture.js","original":{"line":127,"column":15},"generated":{"line":1,"column":5046}},{"source":"views/furniture.js","original":{"line":127,"column":15},"generated":{"line":1,"column":5054}},{"source":"views/furniture.js","original":{"line":127,"column":15},"generated":{"line":1,"column":5062}}],"sources":{"views/furniture.js":"const renderFurnitures = (furnituresList, currentPage, itemsPerPage) => {\r\n    const containerEl = document.querySelector('.furnitures-grid');\r\n    let template;\r\n    if (furnituresList.length) {\r\n        template = furnituresList.slice((currentPage - 1) * itemsPerPage, currentPage * itemsPerPage).map((furniture) => `\r\n        <a class=\"furniture-tile\" href=\"#${furniture.id}\">\r\n            <div class=\"image-container\">\r\n                <img src=\"/assets/images/${furniture.image}\" alt=\"image\">\r\n                <div class=\"stripe\"></div>\r\n            </div>\r\n            <div class=\"name\"><strong>${furniture.name}</strong></div>\r\n            <div class=\"type-price\">\r\n                <span class=\"type\"><strong>${furniture.type}</strong></span>\r\n                <span class=\"price\">$${furniture.price}</span>\r\n            </div>\r\n            <div class=\"ratings-cart\">\r\n                <div class=\"ratings\">${[...Array(5).keys()].map((el) => ((el + 1 <= furniture.rating) ? '<i class=\"fas fa-star\"></i>' : '<i class=\"far fa-star\"></i>')).join(' ')}\r\n                </div>\r\n                <span class=\"cart\"><i class=\"fa fa-cart-plus\" aria-hidden=\"true\"></i></span>\r\n            </div>\r\n        </a>`).join('');\r\n    } else {\r\n        template = '<h2 class=\"text-center\">No furnitures found!</h2>';\r\n    }\r\n    containerEl.innerHTML = template;\r\n};\r\n\r\nconst renderPagination = (totalPages, currentPage) => {\r\n    const paginationContainer = document.querySelector('.pagination');\r\n    if (totalPages) {\r\n        let template = [...Array(totalPages).keys()].map((el) => `\r\n            <span class=\"page-number\" data-goto=\"${el + 1}\">\r\n                <input type=\"radio\" id=\"${el + 1}\" name=\"page\" value=\"${el + 1}\" class=\"page-radio-button\" ${el + 1 === currentPage ? 'checked' : ''}>\r\n                <label for=\"${el + 1}\" class=\"page-label\"><strong>${el + 1}</strong></label>\r\n            </span>\r\n        `).join('');\r\n        template += `\r\n            <span class=\"next-button\">&gt;</span> \r\n        `;\r\n        paginationContainer.innerHTML = template;\r\n    } else {\r\n        paginationContainer.innerHTML = '';\r\n    }\r\n};\r\n\r\nconst renderFurnituresListPage = () => {\r\n    document.querySelector('main .container').innerHTML = `\r\n        <div class=\"layout\">\r\n            <!-- FILTERS SECTION -->\r\n            <div class=\"filters\">\r\n                <h4 class=\"title\">FILTER BY</h4>\r\n                <div class=\"filter-options\">\r\n                    <div class=\"filter-option\">\r\n                        <span>Collection</span>\r\n                        <i class=\"fa fa-angle-down\" aria-hidden=\"true\"></i>\r\n                    </div>\r\n                    <div class=\"filter-option\">\r\n                        <span>Color</span>\r\n                        <i class=\"fa fa-angle-down\" aria-hidden=\"true\"></i>\r\n                    </div>\r\n                    <div class=\"filter-option\">\r\n                        <span>Category</span>\r\n                        <i class=\"fa fa-angle-down\" aria-hidden=\"true\"></i>\r\n                    </div>\r\n                    <div class=\"filter-option-price\">\r\n                        <span class=\"label\">Price Range</span>\r\n                        <input type=\"range\" class=\"slider\" id=\"price-slider\" name=\"price-slider\">\r\n                        <div class=\"range-value\">\r\n                            <span>$1300</span>\r\n                            <span>$10,000+</span>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n            <!-- FURNITURES GRID SECTION -->\r\n            <div class=\"furnitures-grid\"></div>\r\n        </div>\r\n        <!-- PAGINATION -->\r\n        <div class=\"pagination\"></div>\r\n    `;\r\n};\r\n\r\nconst renderFurniturePage = (furnitureData) => {\r\n    document.querySelector('main .container').innerHTML = `\r\n        <div class=\"furniture-page\">\r\n            <div class=\"furniture-image\">\r\n                <img src=\"/assets/images/${furnitureData.image}\" alt=\"image\">\r\n            </div>\r\n            <div class=\"arrow\">\r\n                <i class=\"fas fa-arrow-right\"></i>\r\n            </div>\r\n            <div class=\"furniture-details\">\r\n                <h4 class=\"type\">${furnitureData.type}</h4>\r\n                <h2 class=\"name\">${furnitureData.name}</h2>\r\n                <p class=\"description\">${furnitureData.description}</p>\r\n                <h4 class=\"ppu-text\">Price per unit</h4>\r\n                <div class=\"options\">\r\n                    <h1 class=\"price\">$${furnitureData.price}</h1>\r\n                    <span class=\"buy-now\">Buy Now</span>\r\n                    <span class=\"cart grey-text\"><i class=\"fa fa-cart-plus\" aria-hidden=\"true\"></i></span>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    `;\r\n};\r\n\r\nconst showError = () => {\r\n    document.querySelector('main .container').innerHTML = '<h2 class=\"text-center\">Oops! Something went wrong.</h2>';\r\n};\r\n\r\nconst showLoader = () => {\r\n    document.querySelector('.loader-container').style.visibility = 'visible';\r\n};\r\n\r\nconst hideLoader = () => {\r\n    document.querySelector('.loader-container').style.visibility = 'hidden';\r\n};\r\n\r\nconst showSearch = () => {\r\n    document.querySelector('.search-input').style.visibility = 'visible';\r\n};\r\n\r\nconst hideSearch = () => {\r\n    document.querySelector('.search-input').style.visibility = 'hidden';\r\n};\r\n\r\nexport default {\r\n    renderFurnitures, renderPagination, renderFurnituresListPage, renderFurniturePage, showError, showLoader, hideLoader, showSearch, hideSearch,\r\n};\r\n"},"lineCount":null}},"error":null,"hash":"873c077e5570a27a75d21a34d310e94a","cacheData":{"env":{}}}