ht: 0.5rem;\r\n                }\r\n\r\n                .menu-icon {\r\n                    display: flex;\r\n                    flex-direction: column;\r\n                    height: 100%;\r\n                    width: 60px;\r\n                    min-width: 60px;\r\n                    padding: 18px;\r\n                    background-color: $black-color;\r\n\r\n                    .line {\r\n                        width: 100%;\r\n                        height: 0.25rem;\r\n                        background-color: $white-color;\r\n                        margin-bottom: 4px;\r\n                        border-radius: 1px;\r\n\r\n                        &-half {\r\n                            width: 50%;\r\n                            height: 0.25rem;\r\n                            background-color: $white-color;\r\n                            margin-bottom: 4px;\r\n                            border-radius: 1px;\r\n                        }\r\n                    }\r\n                }\r\n            }\r\n        }\r\n\r\n        .search-part {\r\n            display: flex;\r\n            justify-content: flex-end;\r\n            align-items: center;\r\n            width: 70%;\r\n\r\n            &>span {\r\n                margin-left: 2rem;\r\n\r\n                .fa-search {\r\n                    margin-right: 0.5rem;\r\n                }\r\n\r\n                input {\r\n                    border: none;\r\n                    border-bottom: 1px solid $black-color;\r\n                    padding: 0.5rem 0;\r\n                    font-weight: bold;\r\n                    outline: none;\r\n\r\n                    &:hover,\r\n                    &:active,\r\n                    &:focus {\r\n                        border-bottom: 1px solid $primary-color;\r\n                    }\r\n                }\r\n            }\r\n\r\n\r\n        }\r\n    }\r\n\r\n    @media screen and (max-width: 768px) {\r\n        margin-top: 5%;\r\n    }\r\n}\r\n\r\nmain {\r\n    .container {\r\n        display: grid;\r\n        grid-template-columns: 1fr 3fr;\r\n        grid-gap: 2rem;\r\n\r\n        /* FILTERS STYLES */\r\n        .filters {\r\n            .title {\r\n                @extend .grey-text;\r\n                margin-bottom: 1.5rem;\r\n            }\r\n\r\n            .filter-option {\r\n                display: flex;\r\n                justify-content: space-between;\r\n                align-items: center;\r\n                padding: 0.75rem 0;\r\n                margin-bottom: 0.5rem;\r\n                border-bottom: 1.25px solid $grey-color;\r\n                cursor: pointer;\r\n\r\n                &-price {\r\n                    display: flex;\r\n                    flex-direction: column;\r\n                    padding: 0.75rem 0;\r\n\r\n                    .label {\r\n                        margin-bottom: 1.5rem;\r\n                    }\r\n\r\n                    .slider {\r\n                        -webkit-appearance: none;\r\n                        outline: none;\r\n                        background-color: $grey-color;\r\n                        height: 1.5px;\r\n                        margin-bottom: 1.5rem;\r\n\r\n                        &::-webkit-slider-thumb {\r\n                            -webkit-appearance: none;\r\n                            background-color: $white-color;\r\n                            width: 15px;\r\n                            height: 15px;\r\n                            border: 2px solid black;\r\n                            border-radius: 50%;\r\n                            cursor: pointer;\r\n                        }\r\n                    }\r\n\r\n                    .range-value {\r\n                        display: flex;\r\n                        justify-content: space-between;\r\n                        align-items: center;\r\n                    }\r\n                }\r\n            }\r\n        }\r\n\r\n        /* FURNITURES GRID STYLES */\r\n        .furnitures-grid {\r\n            display: grid;\r\n            grid-template-columns: repeat(3, 1fr);\r\n            grid-gap: 2rem;\r\n\r\n            .furniture-tile {\r\n                .image-container {\r\n                    position: relative;\r\n                    cursor: pointer;\r\n\r\n                    img {\r\n                        width: 100%;\r\n                        height: auto;\r\n                        min-height: 150px;\r\n                    }\r\n                }\r\n\r\n                .stripe {\r\n                    position: absolute;\r\n                    width: 65%;\r\n                    height: 2rem;\r\n                    background-color: $white-color;\r\n                    bottom: 6px;\r\n                }\r\n\r\n                .type-price {\r\n                    display: flex;\r\n                    justify-content: space-between;\r\n                    align-items: center;\r\n                    margin-bottom: 0.5rem;\r\n\r\n                    .type {\r\n                        color: $grey-color;\r\n                        font-size: 12px;\r\n                    }\r\n                }\r\n\r\n                .ratings-cart {\r\n                    display: flex;\r\n                    justify-content: space-between;\r\n                    align-items: center;\r\n\r\n                    .ratings {\r\n                        font-size: 12px;\r\n                    }\r\n                }\r\n            }\r\n        }\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"3a3e2c2fdff6189ca1f46b9e5e18bc0c","cacheData":{"env":{}}}